import java.util.*;
public class Five{
public static void main(String args[])
{
 Scanner sc=new Scanner (System.in);
 int n1,n2,n3,count=0,i=2,k;
 n1=sc.nextInt();
 n2=sc.nextInt();
 n3=sc.nextInt();
  k=sc.nextInt();
 while(count<k)
{
  if((i%n1==0)&&(i%n2==0)&&(i%n3==0))
  {
    System.out.println(i);
    count++;
  }
  i++;
}
}
}
